<div class="container">
  <div class="col-lg-12">
        <%= debug @contacts %>      
    <table class="table table-striped table-center table-hover">    
      <% @contacts.each do |contact| %>
        <tr >
          <td><%= contact.name %></td>
          <td><%= contact.email %></td>
          <td><%= contact.celphone %></td>
        </tr>
      <% end %>  
    </table>
  </div>
</div>